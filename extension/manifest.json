{
  "manifest_version": 2,
  "name": "futakoburakuda",
  "version": "1.0.0",
  "icons": {
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "content_scripts": [{
    "matches": ["https://www.facebook.com/*"],
    "js": ["app.js"]
  }],
  "browser_action": {
    "default_icon": "images/icon_32.png",
    "default_title": "demo"
  }
}